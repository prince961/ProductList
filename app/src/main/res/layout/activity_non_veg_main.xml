<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_non_veg_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="1dp"
    android:paddingRight="1dp"
    android:paddingTop="0dp"
    tools:context="com.example.mohit.productlist.NonVegMain">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/nonveg"
        android:scaleType="fitXY"
        android:id="@+id/imageviewNonveg"/>

    <Button
        android:text="Button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageviewNonveg"
        android:layout_centerHorizontal="true"
        android:id="@+id/button"
        android:onClick="goToCart"
         />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/nonvegmainlistView"
        android:layout_below="@+id/button">

    </ListView>


</RelativeLayout>
